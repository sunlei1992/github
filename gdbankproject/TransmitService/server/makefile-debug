debug_server:server.o decs.o md5c.o
	gcc decs.o md5c.o mac_md5.c server.o -o debug_server -I /usr/include/mysql -L /usr/lib64/mysql  -lmysqlclient -lm 
server.o:server.c server.h
	gcc -c server.c server.h  -I /usr/include/mysql -L /usr/lib64/mysql  -lmysqlclient -lm 
md5c.o: md5c.c md5.h
	gcc -fPIC  -g -c md5c.c -I /usr/include/mysql -L /usr/lib64/mysql  -lmysqlclient 
decs.o:
	gcc -c decs.c -I /usr/include/mysql -L /usr/lib64/mysql  -lmysqlclient 
clean:
	rm -f debug_server *.o *.h.gch
